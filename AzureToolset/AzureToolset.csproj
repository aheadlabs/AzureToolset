<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<Nullable>enable</Nullable>
		<Version>0.0.1</Version>
		<Company>Ahead Labs, S.L.</Company>
		<Authors>Ahead Labs, S.L.</Authors>
		<Description>Opinionated repository that contains common .NET tools and patterns to interact with Azure APIs.</Description>
		<Copyright>Ahead Labs, S.L.</Copyright>
		<PackageProjectUrl>https://github.com/aheadlabs/AzureToolset</PackageProjectUrl>
		<RepositoryUrl>https://github.com/aheadlabs/AzureToolset</RepositoryUrl>
		<RepositoryType>Git</RepositoryType>
		<PackageTags>azure, toolset</PackageTags>
		<NeutralLanguage>en-US</NeutralLanguage>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

</Project>
